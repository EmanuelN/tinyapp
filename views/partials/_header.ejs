<!-- views/partials/header.ejs -->

<h1>
    TinyApp
</h1>

<!-- login form -->
<% if (user) { %>
<p>Currently logged in as: <%= user.email %> </p>
<form method="POST" action="/logout">
  <button name="logout">Logout</button>
</form>
<% } else { %>
<a href="/register">Register</a>
<a href="/login">Login</a>
<% } %>